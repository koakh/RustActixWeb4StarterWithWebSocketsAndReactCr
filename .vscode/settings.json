{
  "peacock.remoteColor": "#aab002",
  "files.watcherExclude": {
    "**/.dart_tool": true,
    "**/.git/objects/**": true,
    "**/.git/subtree-cache/**": true,
    "**/node_modules/*/**": true,
    ".flatpak": true,
    ".flatpak/**": true,
    "_build/**": true
  },
  "cSpell.words": [
    "actix",
    "actixweb",
    "browserslist",
    "chrono",
    "httpauth",
    "Kiwix",
    "linemux",
    "microcloud",
    "Muxed",
    "pathbuf",
    "Postbackup",
    "rsyslog",
    "rtype",
    "structopt",
    "syncthing",
    "tailwindcss",
    "threadpool",
    "websockets"
  ],
  "workbench.colorCustomizations": {
    "activityBar.activeBackground": "#dbe203",
    "activityBar.activeBorder": "#02989d",
    "activityBar.background": "#dbe203",
    "activityBar.foreground": "#15202b",
    "activityBar.inactiveForeground": "#15202b99",
    "activityBarBadge.background": "#02989d",
    "activityBarBadge.foreground": "#e7e7e7",
    "sash.hoverBorder": "#dbe203"
  }
}